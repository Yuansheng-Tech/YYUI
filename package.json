{
  "name": "@ysyp/ui",
  "version": "0.3.10",
  "description": "Taro UI for e-commerce system.",
  "keywords": [
    "taro",
    "taro3",
    "ui",
    "components",
    "react",
    "reactjs",
    "wx",
    "weixin",
    "wechat",
    "weapp",
    "wechat-mini-program",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/Yuansheng-Tech/YYUI",
  "bugs": {
    "url": "https://github.com/Yuansheng-Tech/YYUI/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuansheng-Tech/YYUI.git"
  },
  "license": "MIT",
  "author": "Fu Yin",
  "exports": {
    "require": "./index.js",
    "import": "./dist/index.esm.js",
    "default": "./dist/index.esm.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "del lib/* dist/* tsconfig.tsbuildinfo",
    "build": "yarn run clean && yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:rollup": "rollup --config ./rollup.config.js",
    "clean": "rimraf .temp dist lib coverage",
    "docs": "typedoc --out docs src/index.ts",
    "release": "np",
    "start": "tsc -w --preserveWatchOutput",
    "test": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -x @commitlint/config-conventional -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,jsx,ts,tsx,md,html,css,scss}": "prettier --write"
  },
  "browserslist": {
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-json": "^4.1.0",
    "@skypack/package-check": "^0.2.2",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.0",
    "@types/react": "^16.2.0",
    "@types/react-is": "^16.2.0",
    "del-cli": "^3.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "mobx-react-lite": "^3.2.0",
    "node-sass-chokidar": "^1.5.0",
    "np": "^7.5.0",
    "path": "^0.12.7",
    "prettier": "^2.3.0",
    "prettier-plugin-jsdoc": "^0.3.22",
    "prettier-plugin-packagejson": "^2.2.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.52.8",
    "threejs-miniprogram": "0.0.8",
    "tsccss": "0.0.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@tarojs/components": "^3.3.9",
    "@tarojs/taro": "^3.3.9",
    "@ysyp/stores": "latest",
    "@ysyp/utils": "latest",
    "classnames": "^2.3.1",
    "taro-ui": "^3.0.0-alpha.10",
    "ts-transformer-keys": "^0.4.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "yarn": false,
    "contents": "."
  }
}
